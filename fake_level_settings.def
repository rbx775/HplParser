
<Classes>
	<BaseClass />
    <Class Name="LevelSettings">
        <TypeVars>
            <Group Name="SkyBox">
                <Var Name="SkyBoxActive" Type="Bool" DefaultValue="false" Description="Toggle SkyBox on/off." />
                <Var Name="SkyBoxColor" Type="Color" DefaultValue="1.0 1.0 1.0 1.0" Description="SkyBox color (RGBA)." />
                <Var Name="SkyBoxTexture" Type="String" DefaultValue="" Description="SkyBox texture path." />
                <Var Name="SkyBoxBrightness" Type="Float" DefaultValue="1.0" Min="0.0" Max="1.0" Description="SkyBox brightness." />
            </Group>
            <Group Name="Fog">
                <Var Name="FogActive" Type="Bool" DefaultValue="false" Description="Toggle fog on/off." />
                <Var Name="FogCulling" Type="Bool" DefaultValue="true" Description="Toggle fog culling on/off." />
                <Var Name="FogUnderwater" Type="Bool" DefaultValue="false" Description="Toggle fog underwater on/off." />
                <Var Name="FogLighten" Type="Bool" DefaultValue="true" Description="Toggle fog lightening on/off." />
                <Var Name="FogExponential" Type="Bool" DefaultValue="true" Description="Toggle exponential fog on/off." />
                <Var Name="FogHeightBased" Type="Bool" DefaultValue="false" Description="Toggle height-based fog on/off." />
                <Var Name="FogUseSkybox" Type="Bool" DefaultValue="true" Description="Toggle fog use of skybox on/off." />
                <Var Name="FogApplyAfterFogAreas" Type="Bool" DefaultValue="true" Description="Toggle fog application after fog areas on/off." />
                <Var Name="FogFadeStart" Type="Float" DefaultValue="0.0" Min="0.0" Max="100.0" Description="Fog fade start distance." />
                <Var Name="FogFadeEnd" Type="Float" DefaultValue="20.0" Min="0.0" Max="100.0" Description="Fog fade end distance." />
                <Var Name="FogFalloffExp" Type="Float" DefaultValue="1.0" Min="0.0" Max="10.0" Description="Fog falloff exponent." />
                <Var Name="FogDensity" Type="Float" DefaultValue="0.06" Min="0.0" Max="1.0" Description="Fog density." />
                <Var Name="FogHeightDensity" Type="Float" DefaultValue="0.01" Min="0.0" Max="1.0" Description="Fog height density." />
                <Var Name="FogHeightHorizon" Type="Float" DefaultValue="5.0" Min="0.0" Max="10.0" Description="Fog height horizon distance." />
                <Var Name="FogColor" Type="Color" DefaultValue="1.0 1.0 1.0 1.0" Description="Fog color (RGBA)." />
                <Var Name="FogBrightness" Type="Float" DefaultValue="1.0" Description="Fog brightness." />
                <Var Name="FogNoiseStrength" Type="Float" DefaultValue="0.0" Min="0.0" Max="1.0" Description="Underwater Fog noise strength." />
                <Var Name="FogNoiseSize" Type="Float" DefaultValue="8.0" Min="0.0" Max="10.0" Description="Underwater Fog noise size." />
                <Var Name="FogNoiseTurbulence" Type="Color" DefaultValue="0.5 0.5 0.5" Min="0.0" Max="1.0" Description="Underwater Fog noise turbulence (XYZ)." />
            </Group>
            <Group Name="SecondaryFog">
                <Var Name="SecondaryFogSecondaryActive" Type="Bool" DefaultValue="false" Description="Toggle secondary fog on/off." />
                <Var Name="SecondaryFogSecondaryFadeStart" Type="Float" DefaultValue="0.0" Min="0.0" Max="100.0" Description="Secondary fog fade start distance." />
                <Var Name="SecondaryFogSecondaryFadeEnd" Type="Float" DefaultValue="20.0" Min="0.0" Max="100.0" Description="Secondary fog fade end distance." />
                <Var Name="SecondaryFogSecondaryFalloffExp" Type="Float" DefaultValue="1.0" Min="0.0" Max="10.0" Description="Secondary fog falloff exponent." />
                <Var Name="SecondaryFogSecondaryDensity" Type="Float" DefaultValue="0.01" Min="0.0" Max="1.0" Description="Secondary fog density." />
                <Var Name="SecondaryFogSecondaryHeightDensity" Type="Float" DefaultValue="0.25" Min="0.0" Max="1.0" Description="Secondary fog height density." />
                <Var Name="SecondaryFogSecondaryHeightHorizon" Type="Float" DefaultValue="5.0" Min="0.0" Max="10.0" Description="Secondary fog height horizon distance." />
                <Var Name="SecondaryFogSecondaryColor" Type="Color" DefaultValue="1.0 1.0 1.0 1.0" Description="Secondary fog color (RGBA)." />
            </Group>
            <Group Name="Decals">
                <Var Name="GlobalMaxTrianglePerDecal" Type="Int" DefaultValue="300" Description="Number of triangles per decal." />
            </Group>
