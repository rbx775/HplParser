                <Var Name="PostEffectsToneMappingKey" Type="Float" DefaultValue="0.5" Min="0.0" Max="1.0" Description="Tone mapping key for post-effects." />
                <Var Name="PostEffectsToneMappingExposure" Type="Float" DefaultValue="0.0" Description="Tone mapping exposure for post-effects." />
                <Var Name="PostEffectsToneMappingWhiteCut" Type="Float" DefaultValue="3.5" Min="0.0" Max="10.0" Description="Tone mapping white cut for post-effects." />
                <Var Name="PostEffectsColorGradingTexture" Type="String" DefaultValue="textures/colorgrading/bunker_prototype_01.dds" Description="Color grading texture path for post-effects." />
                <Var Name="PostEffectsNormalMap" Type="Bool" DefaultValue="false" Description="Toggle SSAO normal map on/off." />
                <Var Name="PostEffectsNumOfDirections" Type="Int" DefaultValue="0" Description="Number of SSAO directions." />
                <Var Name="PostEffectsNumOfSteps" Type="Int" DefaultValue="0" Description="Number of SSAO steps." />
