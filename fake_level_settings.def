                <Var Name="PostEffectsPower" Type="Float" DefaultValue="4.4" Min="0.0" Max="10.0" Description="SSAO power." />
                <Var Name="PostEffectsRadius" Type="Float" DefaultValue="2.0" Min="0.0" Max="10.0" Description="SSAO radius." />
                <Var Name="PostEffectsBufferSizeDiv" Type="Int" DefaultValue="2" Min="1" Max="10" Description="SSAO buffer size divisor." />
                <Var Name="PostEffectsAngleBias" Type="Float" DefaultValue="0.0" Description="SSAO angle bias." />
                <Var Name="PostEffectsPower" Type="Float" DefaultValue="4.4" Description="SSAO power." />
                <Var Name="PostEffectsRadius" Type="Float" DefaultValue="2.0" Description="SSAO sampling radius." />
                <Var Name="PostEffectsBufferSizeDiv" Type="Int" DefaultValue="2" Description="SSAO buffer size divisor." />
            </Group>
