                <Var Name="EnvironmentParticlesFadeOutStart" Type="Float" DefaultValue="10.0" Min="0.0" Max="100.0" Description="Fade-out start time for the environment particle." />
                <Var Name="EnvironmentParticlesFadeOutEnd" Type="Float" DefaultValue="20.0" Min="0.0" Max="100.0" Description="Fade-out end time for the environment particle." />
