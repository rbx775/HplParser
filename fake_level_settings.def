                <Var Name="EnvironmentParticlesGravitySpeedRandomAmount" Type="Float" DefaultValue="0.0" Min="0.0" Max="1.0" Description="Gravity speed random amount of the environment particle." />
                <Var Name="EnvironmentParticlesWindVelocity" Type="Vector3" DefaultValue="0.0 0.0 0.0" Description="Wind velocity of the environment particle (XYZ)." />
