                <Var Name="EnvironmentParticlesNumParticles" Type="Int" DefaultValue="100" Min="1" Max="1000" Description="Number of particles for the environment particle." />
                <Var Name="EnvironmentParticlesParticleSize" Type="Vector2" DefaultValue="1.0 1.0" Description="Particle size of the environment particle (XY)." />
                <Var Name="EnvironmentParticlesSubDivUV" Type="Vector2" DefaultValue="1.0 1.0" Description="Subdivision UV of the environment particle (XY)." />
                <Var Name="EnvironmentParticlesAffectedByLight" Type="Bool" DefaultValue="false" Description="Toggle light effect on the environment particle on/off." />
                <Var Name="EnvironmentParticlesTexture" Type="String" DefaultValue="particles/clouds/materials/clouds_midday.dds" Description="Texture path for the environment particle." />
                <Var Name="EnvironmentParticlesVisible" Type="Bool" DefaultValue="true" Description="Toggle visibility of the environment particle on/off." />
