                <Var Name="EnvironmentParticlesWindDirRandomAmount" Type="Float" DefaultValue="0.0" Min="0.0" Max="1.0" Description="Wind direction random amount of the environment particle." />
                <Var Name="EnvironmentParticlesRotateVelocity" Type="Vector3" DefaultValue="0.0 0.0 0.0" Description="Rotate velocity of the environment particle (XYZ)." />
                <Var Name="EnvironmentParticlesRotateSpeedRandomAmount" Type="Float" DefaultValue="0.0" Min="0.0" Max="1.0" Description="Rotate speed random amount of the environment particle." />
