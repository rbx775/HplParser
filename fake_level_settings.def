                <Var Name="DirectionalLightShadowMapSlopeScaleBiasMul" Type="Float" DefaultValue="2.5" Min="0.0" Max="10.0" Description="Shadow map slope scale bias multiplier for the directional light." />
                <Var Name="DirectionalLightShadowDistance" Type="Float" DefaultValue="-1.0" Min="-1.0" Max="100.0" Description="Shadow distance for the directional light." />
                <Var Name="DirectionalLightShadowCasterDist" Type="Float" DefaultValue="35.0" Min="0.0" Max="100.0" Description="Shadow caster distance for the directional light." />
                <Var Name="DirectionalLightAutoShadowSliceSettings" Type="Bool" DefaultValue="true" Description="Toggle auto shadow slice settings for the directional light on/off." />
                <Var Name="DirectionalLightAutoShadowSliceLogTerm" Type="Float" DefaultValue="0.9" Min="0.0" Max="1.0" Description="Auto shadow slice log term for the directional light." />
            </Group>
            <Group Name="EnvironmentParticles">
                <Var Name="EnvironmentParticlesActive" Type="Bool" DefaultValue="false" Description="Toggle environment particles on/off." />
